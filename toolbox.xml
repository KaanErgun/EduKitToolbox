<xml id="toolbox" style="display: none">
    <category name="Temel" colour="#5CA65C">
        <block type="controls_if"></block>
        <block type="controls_repeat_ext">
            <value name="TIMES">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="logic_compare"></block>
        <block type="math_arithmetic"></block>
        <block type="text"></block>
        <block type="text_print"></block>
    </category>
    <category name="Matematik" colour="#5C68A6">
        <block type="math_number"></block>
        <block type="math_arithmetic"></block>
        <block type="math_random_int">
            <value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="TO">
                <shadow type="math_number">
                    <field name="NUM">100</field>
                </shadow>
            </value>
        </block>
    </category>
    <category name="Değişkenler" colour="#A65C81" custom="VARIABLE"></category>
    <category name="Fonksiyonlar" colour="#9A5CA6" custom="PROCEDURE"></category>
</xml>
